---
layout: null
---

[
  {% for post in site.posts %}
    {
      "title": {{ post.title | jsonify }},
      "url": "{{ site.baseurl }}{{ post.url }}",
      "date": "{{ post.date }}",
      "content": {{ post.content | strip_html | strip_newlines | truncate: 160 | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
